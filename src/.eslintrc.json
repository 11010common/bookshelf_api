{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": "airbnb-base",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"no-console": "off",
		"linebreak-style": "off",
		"object-curly-newline": "off",
		"arrow-body-style": "off",
		"indent": "off",
		"no-tabs": "off",
		"max-len": "off",
		"import/no-extraneous-dependencies": "off",
		"object-curly-spacing": "off",
		"no-unused-vars": "off",
		"consistent-return": "off"
	}
}
